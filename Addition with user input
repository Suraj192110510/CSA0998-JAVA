import java.util.*;
public class inputadd {
    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        System.out.println("Entre 2 numbers to add");
        int a,b,c;
        a=s.nextInt();
        b=s.nextInt();
        c=a+b;
        System.out.println("sum="+c);
    }
}
