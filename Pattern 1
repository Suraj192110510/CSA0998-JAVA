import java.util.Scanner;

class Pattern1
{
    public static void main(String args[])
    {
        Scanner abc = new Scanner(System.in);
        int n = abc.nextInt();
        for(int i = 1; i <= n;i++)
        {
            for(int j = 1;j <= i;j++)
            {
                System.err.print("* ");
            }
            System.out.println();
        }
    }
}
