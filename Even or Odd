public class evenodd {
    public static void main(String[] args){
        int i=5;
        if(i%2==0){
            System.out.println(i+" is Even Number");
        }
        else if(i<0){
            System.out.println(i+" Invalid Valid Input");
        }
        else{
            System.out.println(i+" Odd Number");
        }
    }

}
