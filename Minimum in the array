import java.util.*;
class minarr
{
public static void main(String args[])
{
Scanner s = new Scanner(System.in);

int n=s.nextInt();
int min=0;
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
for(int i=0;i<n;i++)
{
if(i==0)
{
min=a[i];
}
if(min>a[i])
{
min=a[i];
}
}
System.out.print("result is : " + min);
}
}
