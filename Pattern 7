import java.util.Scanner;

class Pattern7
{
    public static void main(String args[])
    {
        Scanner abc4 = new Scanner(System.in);
        int n = abc4.nextInt();
        int k = 1;

        for(int i = 0; i < n;i++)
        {
            for(int j = 0;j <= i;j++)
            {
                System.out.print((k * k) + " ");
                k = k + 1;
            }
            System.out.println();
        }
    }
}
