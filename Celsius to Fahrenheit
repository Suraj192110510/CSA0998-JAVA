import java.util.*;
public class celtofaren {
    public static void main(String[] args) {
    Scanner s=new Scanner(System.in);
    System.out.println("Enter the celcius: ");
    float cel,far;
    cel=s.nextFloat();
    far=cel*(9.0f/5.0f)+32;
    System.out.println("Fareheit: "+far);
        
    }
    
}
